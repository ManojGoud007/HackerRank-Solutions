#include <assert.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main()
{
 long int a[100],sum=0,n,i;
scanf("%ld",&n);
for(i=0;i<n;i++)
{ 
   scanf(" %ld",&a[i]);
}
  for(i=0;i<n;i++)
  {
      sum=sum+a[i];
  }
printf("%ld",sum);
}









