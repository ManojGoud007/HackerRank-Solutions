#include <assert.h>
#include <ctype.h>
#include <limits.h>
#include <math.h>
#include <stdbool.h>
#include <stddef.h>
#include <stdint.h>
#include <stdio.h>
#include <stdlib.h>
#include <string.h>
 int main()
 {
     int a[10],b[10],c[10],n,i,d,j,count=0,flag=0,e;
     for(i=0;i<3;i++)
     {
     scanf("%d",&a[i]);
     }
     for(i=0;i<3;i++)
     {
        scanf("%d",&b[i]);
     }
     for(i=0;i<3;i++)
     {
         if(a[i]>b[i])
             count++;
         else if(b[i]>a[i])
             flag++;

     }
     printf("%d %d",count,flag);
 }
     
 

     
         
